{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "jsx": "react",
    "typeRoots": ["./types", "./node_modules/@types"],
    "strict": true,
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "constants",
    "./types",
    "**/*.ts",
    "**/*.tsx"
  ]
}
